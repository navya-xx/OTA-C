    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) CSD Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/ProjectRoot/build/exp_csd",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
